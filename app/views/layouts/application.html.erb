<!DOCTYPE html>
<html>
  <head>
    <title>MusicApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

  <header>
    <h1>Music App</h1>
    <div>
    <% if logged_in? %>
      <%= link_to "Log Out", session_url, method: :delete %>
    <% else %>
      <%= link_to "Sign Up", new_user_url, method: :get %>
      <%= link_to "Log In", new_session_url, method: :get %>
    <% end %>
    </div>
  </header>


    <%= yield %>
  </body>
</html>
